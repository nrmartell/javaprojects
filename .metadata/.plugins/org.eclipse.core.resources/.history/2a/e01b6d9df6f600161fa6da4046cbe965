package classexamples;

public class STA {

	public class STA { int x, y; static int a, b;
	static int getSumAB(){ return a+b;
	}
	int getSumABCD(){ return a+b+x+y;
	}
	     int getSumXY(){
	       return x+y;
	} }
//	(a) What will be the output of the following program?
	
	public class StAStaticTest {
	public static void main(String args[]){
	 STA s1 = new
	STA s2 = new
	s1.a=10;
	s1.b=s1.a*2;
	STA();
	STA();
	s2.x=30;
	s2.y=s2.x*2;
	s2.a=s2.y+5;
	System.out.println(s1.a+" "+s1.b+" "+s1.x+" "+s1.y); System.out.println(s2.a+" "+s2.b+" "+s2.x+" "+s2.y);
	} }

}
